<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<!-- simple revered totem -->
	<Operation Class="PatchOperationReplace">
		<success>Always</success>
		<xpath>Defs/ThingDef[defName="BTEMy_SimpleReveredTotem"]/label</xpath>
		<value>
			<label>simple totem</label>
		</value>
	</Operation>

	<!-- life filled totem -->
	<Operation Class="PatchOperationReplace">
		<success>Always</success>
		<xpath>Defs/ThingDef[defName="BTEMy_LifeFilledTotem"]/label</xpath>
		<value>
			<label>life totem</label>
		</value>
	</Operation>

</Patch>
