	<!-- Masonry table -->
	<Operation Class="PatchOperationReplace">
		<success>Always</success>
		<xpath>Defs/ThingDef[defName="FT_TableMasonry"]/label</xpath>
		<value>
			<label>masonry table</label>
		</value>
	</Operation>
	
</Patch>
